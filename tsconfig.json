{
  "ts-node": {
    "transpileOnly": true,
    "sourceMap": true,
    "inlineSources": true
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "jsxImportSource": "./handlers/definers/",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "outDir": "./dist-commandfiles",
    "rootDir": "./",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "target": "ES2024",
    "lib": ["ES2024"],
    "types": ["node"],
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "declarationMap": true,
    "allowJs": true,
    "checkJs": false,
    "verbatimModuleSyntax": false,
    "strictNullChecks": false,
    "useUnknownInCatchVariables": false,
    "paths": {
      "@cassidy/unispectra": ["./CommandFiles/modules/unisym"],
      "@cassidy/spectral-home": ["./CommandFiles/modules/spectralCMDHome"],
      "@cassidy/redux-home": ["./CommandFiles/modules/reduxCMDHome"],
      "@cassidy/ut-shop": ["./CommandFiles/plugins/ut-shop"],
      "@cassidy/utils": ["./CommandFiles/types/utils-path"],
      "@cass/unispectra": ["./CommandFiles/modules/unisym"],
      "@cass/spectral-home": ["./CommandFiles/modules/spectralCMDHome"],
      "@cass/redux-home": ["./CommandFiles/modules/reduxCMDHome"],
      "@cass/ut-shop": ["./CommandFiles/plugins/ut-shop"],
      "@cass/utils": ["./CommandFiles/types/utils-path"],
      "@cass-modules/*": ["./CommandFiles/modules/*"],
      "@cass-plugins/*": ["./CommandFiles/plugins/*"],
      "@cass-commands/*": ["./CommandFiles/commands/*"],
      "@cassidy/styler": ["./handlers/styler.js/main"],
      "@cass/define": ["./handlers/definers/defineEntry"],
      "@cass/plugin-order": ["./CommandFiles/pluginOrder.js"],
      "output-cassidy": ["./CommandFiles/types/output.d.ts"],
      "input-cassidy": ["./CommandFiles/types/input.d.ts"],
      "@cass/output-types": ["./CommandFiles/types/output.d.ts"],
      "@cass/input-types": ["./CommandFiles/types/input.d.ts"],
      "@cassidy/output-types": ["./CommandFiles/types/output.d.ts"],
      "@cassidy/input-types": ["./CommandFiles/types/input.d.ts"],
      "@cassidy/polyfills/*": ["./handlers/polyfills/*"],
      "cassidy-userData": ["./CommandFiles/modules/cassidyUser.d.ts"],
      "neax-script": ["./CommandFiles/modules/NeaxScript"],
      "@cassidy/jsx": ["./handlers/definers/jsx-runtime"],
      "@root/*": ["./*"]
    }
  },
  "exclude": ["node_modules", "public"],
  "include": ["**/*"]
}
